function rand() {
const r = Math.random() * (max - min) + min;
return Math.floor(r);
}

const max = 50;
const min = 1;
let num = rand(min, max);

while (num !== 10) {
    num = rand(min, max);
    console.log(num);
}
console.log(`###########`)
do {num = rand(min, max);
    console.log(num);
} while (num !== 10);

